{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/favicon.svg",
      "destination": "/favicon.svg"
    },
    {
      "source": "/portfolio-photo.jpg",
      "destination": "/portfolio-photo.jpg"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
